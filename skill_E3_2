function definePrime(num) {
    let isPrime, result;
    isPrime = true;

    if (num > 1 && num <= 1000) {
        for (let i = 2; i < num; i++) {
            if (num % i === 0) {
                isPrime = false;
            }
        }
        console.log(result = isPrime  ? 'Число ${num} - простое число' : 'Число ${num} - составное число');
    } else if (num > 1000) {
        console.log(result = 'Данные неверны');
    }
    return result;    
}

definePrime(3)
